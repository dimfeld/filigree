{% import "sql_macros.tera" as macros %}
SELECT
  {{ fields
    | filter(attribute='owner_read', value=true)
    | map(attribute='sql_full_name')
    | join(sep=',') }},
FROM
  {{ table }}

{% set p = 1 %}
{% if not global %}
WHERE
    organization_id = ${{ p }}
{% set p = p + 1 %}
{% endif %}
LIMIT ${{ p }}
OFFSET ${{ p + 1 }}

